   <scripts>
      <row>
         <process><![CDATA[!CHANGE 2
! STANDARD INCLUDE FOR LOGIN & HOST
!
:INC .FRM.STD.INC
:INC .FRM.SET_LOGIN.INC
!
!:set &TRANSPORT_FILENAME# = &TRANSPORT_PRJ#_&TRANSPORT_VERSION#_&$PHYS_DATE_YYMMDD#_&$PHYS_TIME_HHMM#.txt
:INC .TRANSPORT.STRIP_DOCU.INC
! !
! ! UNLOAD
! !
! :SET &UNLOAD_CHECK# = &P_UNLOAD_CHECK#
! :SET &LOAD_CHECK#   = &P_LOAD_CHECK#
! !
! :IF &P_UNLOAD_CHECK# = "YES"
! :  SET &SOURCE_CLIENT#     = format(&P_SOURCE_CLIENT#,"0000")
! :ENDIF
! !
! ! COMMENT
! !
! :SET  &TRANSPORT_COMMENT#  =   &P_TRANSPORT_COMMENT#
! !
! ! LOAD
! !
! :IF &P_LOAD_CHECK# = "YES"
! :  SET &TARGET_CLIENT#     = format(&P_TARGET_CLIENT#,"0000")
! :ENDIF
!!! UNLOAD
!
:SET &UNLOAD_CHECK#    = &P_UNLOAD_CHECK#
:SET &LOAD_CHECK#      = &P_LOAD_CHECK#
:SET &ARCHIVE_CLIENTS# = ""
!
:SET &REPORT_TARGET#   = &P_REPORT_TARGET#
!
:IF &P_UNLOAD_CHECK# = "YES"
:  SET &SOURCE_CLIENT#     = format(&P_SOURCE_CLIENT#,"0000")
:  SET &ARCHIVE_CLIENTS# = "SOURCE: &SOURCE_CLIENT# "
:ENDIF
!
! LOAD
!
:IF &P_LOAD_CHECK# = "YES"
:  SET &TARGET_CLIENT#     = format(&P_TARGET_CLIENT#,"0000")
:  SET &ARCHIVE_CLIENTS# = "&ARCHIVE_CLIENTS#TARGET: &TARGET_CLIENT#"
:ENDIF
!
! INCIDENT NR.
!
:SET &INCIDENT_NR#   = &P_INCIDENTNR#
:IF &P_TARGET_CLIENT# = "1"
:  IF &INCIDENT_NR# = ""
:    PRINT "Loading into client 1 requires incident number!"
:    STOP MSG, 50, "Loading into client 1 requires incident number!"
:  ELSE
:    SET &INCIDENT_NR# = str_cat(&INCIDENT_NR#,";")
:    PRINT "Incident number: &INCIDENT_NR#"
:  ENDIF
:ENDIF
!
!
! COMMENT
!
:SET  &TRANSPORT_COMMENT#  =   &P_TRANSPORT_COMMENT#
!
!
! WRITING DEFAULT WITH VERSION 0.0.0 e.g. CRR_0.0.0_161115_1123.txt
!
:PSET &TRANSPORT_FILENAME# = "&TRANSPORT_PROJECT#_0.0.0_000000_0000.txt"
!
:put_att ARCHIVE_KEY2 = "&TRANSPORT_PROJECT#"
:put_att ARCHIVE_KEY1 = "&ARCHIVE_CLIENTS#"
!
!]]></process>
      </row>
