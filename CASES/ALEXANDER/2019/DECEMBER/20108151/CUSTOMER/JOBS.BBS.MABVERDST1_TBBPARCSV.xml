   <scripts>
      <row>
         <process><![CDATA[:SET &ORADB#     = "DLLW1&IDT#"
:SET &ORAUSER#   = "bbsbat"
:SET &ORASCRIPT# = "tbbpar.sql"
:SET &MANDANT#   = ""

cat << __EOF__  > &RUNDIR#/&ORASCRIPT#
SELECT MANDANT,
       PAR_ART,
       LANG_BEZ,
       PAR_WERT,
       WIRKAB,
       WIRKBIS
  FROM TBBPAR
 WHERE PAR_ART IN ('FBP','BPF','SAP','SAL','SAM')
   AND MANDANT IN (&MANDANTEN#)
ORDER BY MANDANT, PAR_ART
;
__EOF__

:INC JOBI.SQL_CSV

exit $rc]]></process>
