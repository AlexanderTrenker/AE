! Sicherstellung der Abfolge:
! GDV-Vermittlerupdate -> DisDa-Export -> DisDa-Import -> GDV-Batch
! -----------------------------------------------------------------
:SET &ME_RUNNR# = SYS_ACT_ME_NR()
:SET &EBENE# = GET_VAR("VARA.CONFIG_EBENE", "EBENE")
:SET &IDT# = STR_CUT(&EBENE#, 1, 1)
