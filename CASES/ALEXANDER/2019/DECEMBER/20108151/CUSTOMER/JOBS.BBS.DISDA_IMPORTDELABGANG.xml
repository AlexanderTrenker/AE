      </row>
   </general_attributes>
   <scripts>
      <row>
         <process><![CDATA[delete
  from tbbvtr_t_stand t1
 where wirksam_zum >=  to_char (add_months( trunc (sysdate, 'MM'), -1), 'YYYYMMDD')
  and not exists (
                  select *
                    from spiegelwork.tbbvtr_t_stand t2
                   where t1.mandant     = t2.mandant
                     and t1.vtr_t_id    = t2.vtr_t_id
                     and t1.wirksam_zum = t2.wirksam_zum
                  )
;]]></process>
      </row>
      <row>
         <pre_process><![CDATA[:IF &EBENE# NE "P"
:  STOP NOMSG, 4711, ">>> Aktivierung aufgrund der Ebene [&EBENE#] abgebrochen ..."
:ENDIF
:INC JOBI.SQL_BBS2_PARAM]]></pre_process>
      </row>
