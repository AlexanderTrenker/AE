   <scripts>
      <row>
         <process><![CDATA[:SET &VARA# = "VARA.BATCH.DISDA_BATCH"
:SWITCH &MODUS#
:  CASE "INITIAL"
:  PSET &DELTA# = "N"
:  IF &VALUTA# = ""
:    PSET &VALUTA# = GET_VAR(&VARA#, "VALUTA", 1)
:  ENDIF
:  CASE "DELTA"
:  PSET &DELTA# = "J"
:  IF &VALUTA# = ""
:    PSET &VALUTA# = GET_VAR(&VARA#, "VALUTA", 1)
:  ENDIF
:  IF &VALUTA_VON# = ""
:    PSET &VALUTA_VON# = GET_VAR(&VARA#, "VALUTA_VON", 1)
:  ENDIF
:  IF &VALUTA_BIS# = ""
:    PSET &VALUTA_BIS# = GET_VAR(&VARA#, "VALUTA_BIS", 1)
:  ENDIF
:  OTHER
:  STOP MSG, 4711, ">>> Aktivierung abgebrochen da MODUS nicht vorgegeben wurde ..."
:ENDSWITCH]]></process>
      </row>
