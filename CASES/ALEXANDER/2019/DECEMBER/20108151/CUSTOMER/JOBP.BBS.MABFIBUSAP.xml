   <scripts>
      <row>
         <process><![CDATA[! BBS Standart-Include zum setzen von Variablen für aufgerufene Jobs
:INC JOBI.BBS.JOBPDEFAULTS
! ---------------------------------------------
! Weitere Variablenbelegung
!
! ---------------------------------------------
!**********GeneratedText, Do Not Change**********
:SET &pttyp# = SYS_ACT_PTTYP()
!Eintragen der READ Befehle
:BEGINREAD
:IF &pttyp# = "D"
:  PRINT "", "Bei Start via GUI wird die Eingabe der Startzeit unterdrückt"
:ELSE
:  READ &START#,,"START [JJJJ-MM-TT HH:MM:SS]",'',K
:ENDIF
:READ &UC4_DEPLOYMENT_TEST#,,"UC4_DEPLOYMENT_TEST [J/N]",'N',K
:READ &MANDANTEN#,,"MANDANTEN [01,02,20,21,54]",'01,02,20,21,54',K
:READ &FIBU_FLAG#,,"FIBU_FLAG [M,JAB,M13]",'',K
:READ &BUCH_PER#,,"BUCH_PER [JJJJMM]",'',K
:READ &COMMIT#,,"COMMIT [J/N]",'J',K
:READ &TRACE#,,"TRACE [J/N]",'N',K
:READ &RUN_VERDST3#,,"RUN_VERDST3 [J/N]",'J',K
:READ &RUN_HBBBSSAP#,,"RUN_HBBBSSAP [J/N]",'J',K
:READ &RUN_BBS2SAP#,,"RUN_BBS2SAP [J/N]",'J',K
:READ &RUN_BUCHJOUR#,,"RUN_BUCHJOUR [J/N]",'J',K
:READ &RUN_COGNOS#,,"RUN_COGNOS [J/N]",'J',K
:ENDREAD

!Eintragen der PSET Befehle
:PSET &START# = &START#
:PSET &UC4_DEPLOYMENT_TEST# = &UC4_DEPLOYMENT_TEST#
:PSET &MANDANTEN# = &MANDANTEN#
:PSET &FIBU_FLAG# = &FIBU_FLAG#
:PSET &BUCH_PER# = &BUCH_PER#
:PSET &COMMIT# = &COMMIT#
:PSET &TRACE# = &TRACE#
:PSET &RUN_VERDST3# = &RUN_VERDST3#
:PSET &RUN_HBBBSSAP# = &RUN_HBBBSSAP#
:PSET &RUN_BBS2SAP# = &RUN_BBS2SAP#
:PSET &RUN_BUCHJOUR# = &RUN_BUCHJOUR#
:PSET &RUN_COGNOS# = &RUN_COGNOS#
!**************End of GeneratedText**************
:SET &LDATE# = SYS_LDATE("JJJJMMTT")
:SET &LYEAR# = SUBSTR(&LDATE#, 1, 4)
:SET &LMONTH# = SUBSTR(&LDATE#, 5, 2)

:IF &FIBU_FLAG# = ""
:  SET &TREFFER# = CALE_LOOK_AHEAD("JJJJMMTT:&LDATE#", "ONE", "CALE.BBS.BATCH", "BBS.FIBU_13.G")
:  IF &TREFFER# EQ &LDATE#
:    PSET &FIBU_FLAG# = "M13"
:  ENDIF
:  SET &TREFFER# = CALE_LOOK_AHEAD("JJJJMMTT:&LDATE#", "ONE", "CALE.JAB", "BBS.JAB.S")
:  IF &TREFFER# EQ &LDATE#
:    PSET &FIBU_FLAG# = "JAB"
:  ENDIF
:  SET &TREFFER# = CALE_LOOK_AHEAD("JJJJMMTT:&LDATE#", "ONE", "CALE.BBS.BATCH", "BBS.FIBU.G")
:  IF &TREFFER# EQ &LDATE#
:    PSET &FIBU_FLAG# = "M"
:  ENDIF
:ENDIF
:IF &FIBU_FLAG# = ""
:  STOP MSG, 50, "-> FIBU Flag weder per Parameter noch per Kalender definiert!"
:ENDIF

:IF &BUCH_PER# = ""
:  IF &FIBU_FLAG# = "M13"
:     PSET &BUCH_PER# = "&LYEAR#13"
:  ENDIF
:ENDIF
]]></process>
