   <scripts>
      <row>
         <process>R3_GET_VARIANT_CONTENTS REPORT="ZZLWAFILECOPY2",VARIANT="BUKAS-PROD"</process>
      </row>
      <row>
         <pre_process><![CDATA[!Standards einlesen
]]></pre_process>
      </row>
      <row>
         <post_process><![CDATA[:SET &LINE# = ''
:SET &HND# = PREP_PROCESS_REPORT(,,PLOG,'*PA_IASF *','COL=DELIMITER','DELIMITER=*;*')
:PROCESS &HND#
:  SET &LINE# = GET_PROCESS_LINE(&HND#,5)
:  IF &LINE# <> ''
:    TERM_PROCESS
:  ENDIF
:ENDPROCESS
:CLOSE_PROCESS &HND#

:SET  &POS# = STR_FIND_REV('&LINE#', '/')
:PSET &DESTDIR# = STR_CUT(&LINE#, 1, &POS#)

]]></post_process>
      </row>
