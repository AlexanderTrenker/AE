         <type>SQLI_SEC</type>
         <sql><![CDATA[SELECT OH_Name, 
(CASE HOST_Active
         WHEN 1 then 'Inactive'
         WHEN 0 then 'Running'
END)as STATUS,1 as Count
FROM OH, HOST
WHERE OH_Idnr = HOST_OH_Idnr
AND OH_Otype ='SERV'
AND OH_Name NOT LIKE '<%>']]></sql>
