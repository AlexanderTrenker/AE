   <scripts>
      <row>
         <process><![CDATA[:SET &UTILITY# = "ucybdbrr"
! Common Setup
:INC JOBI.UTILITY.SETUP

! Utility Specific
:PUT_ATT ARCHIVE_KEY1 = &CLIENT#
:SET &OUTFILE# = "&OUTDIR#REVISIONS.&$RESTART_RUNID#.xls"

:SET &V_SDATE# = STR_SUB(&SDATE#,' ','')
:SET &V_SDATE# = STR_SUB(&V_SDATE#,'-','')
:SET &V_SDATE# = STR_SUB(&V_SDATE#,':','')
:SET &V_EDATE# = STR_SUB(&EDATE#,' ','')
:SET &V_EDATE# = STR_SUB(&V_EDATE#,'-','')
:SET &V_EDATE# = STR_SUB(&V_EDATE#,':','')
:IF &RTYPES# <> ""
:  SET &TYPES# = "-F&RTYPES#"
:ELSE
:  SET &TYPES# = ""
:ENDIF

:SET &UTILPARMS# = "-I&INIFILE# -C&CLIENT# -B &TYPES# -O&OUTFILE# -D1&V_SDATE# -D2&V_EDATE#"

! Execute Utility
:INC JOBI.UTILITY.EXECUTE]]></process>
      </row>
