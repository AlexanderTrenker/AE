         <type>SQLI_SEC</type>
         <sql><![CDATA[select oh_name 
from oh 
inner join host on host_oh_idnr = oh_idnr
where oh_otype = 'HOST' and oh_idnr > 100000 and oh_deleteflag = 0 and 
  oh_idnr in (select hacl_oh_idnr from hacl where hacl_client = ?)
order by oh_name]]></sql>
