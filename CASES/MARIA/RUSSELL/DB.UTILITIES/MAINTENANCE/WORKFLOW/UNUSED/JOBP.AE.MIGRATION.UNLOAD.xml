   <scripts>
      <row>
         <process><![CDATA[:set &nl# = uc_crlf

:if &UNLOAD_CLIENT# = "TRUE"
:  set &S_MODE# = "TRANSPORTALL"
:else
:  set &S_MODE# = "TRANSPORT"
:endif
:publish &S_MODE#,,"TASK"

:set &environ_vara# = get_var(VARA.MAINTENANCE,ENVIRON_VARA,1)
:set &S_SYSTEM#     = get_var(&environ_vara#,&S_ENVIRONMENT#,1)
:set &S_CLIENT#     = get_var(&environ_vara#,&S_ENVIRONMENT#,2)

:if &S_SYSTEM# = ""
:  P &nl# *** ERROR: No system found for environment &S_ENVIRONMENT# *** &nl#
:  exit 1
:endif

:if &S_CLIENT# = ""
:  P &nl# *** ERROR: No client found for environment &S_ENVIRONMENT# *** &nl#
:  exit 1
:endif

:publish &S_SYSTEM#,,"TASK"
:publish &S_CLIENT#,,"TASK"


:if "&S_SYSTEM#-&S_MODE#" = "&$SYSTEM#-TRANSPORT"
:  set &lf# = uc_crlf
:  set &cnt# = get_var(VARA.SEC_SQLI.TRANSPORT_COUNT)
:  p &lf# Client &S_CLIENT# transport case count: &cnt#
:  if &cnt# < 1
:    p &lf# ERROR: There are no objects in the transport case &lf#
:    exit 1
:  endif
:endif]]></process>
      </row>
