   <scripts>
      <row>
         <process><![CDATA[:SET &UTILITY# = "ucybdbld"
! Common Setup
:INC JOBI.UTILITY.SETUP

! Utility Specific
:PUT_ATT ARCHIVE_KEY1 = &CLIENT#

:SET &@MESSAGES# = &MESSAGES#
:SET &@READ_DAYS# = FORMAT(&READ_DAYS#,"0000")
:SET &@UNREAD_DAYS# = FORMAT(&UNREAD_DAYS#,"0000")
:SET &@MELD# = "&@MESSAGES# &@READ_DAYS# &@UNREAD_DAYS#"
:SET &@MELD_LEN# = STR_LENGTH(&@MELD#)
:SET &@MELD_LEN# = FORMAT(&@MELD_LEN#,"000000000")

:SET &@STATISTICS# = &STATISTICS#
:SET &@STATISTICS_DAYS# = FORMAT(&STATISTICS_DAYS#,"0000")
:SET &@STATISTICS_OBJ# = &STATISTICS_OBJ#
:SET &@STATISTICS_OBJ_DAYS# = FORMAT(&STATISTICS_OBJ_DAYS#,"0000")
:SET &@AH# = "&@STATISTICS# &@STATISTICS_DAYS# &@STATISTICS_OBJ# &@STATISTICS_OBJ_DAYS#"
:SET &@AH_LEN# = STR_LENGTH(&@AH#)
:SET &@AH_LEN# = FORMAT(&@AH_LEN#,"000000000")

:SET &@REPORTS# = &REPORTS#
:SET &@REPORT_DAYS# = FORMAT(&REPORT_DAYS#,"0000")
:SET &@REPORTS_OBJ# = &REPORTS_OBJ#
:SET &@REPORTS_OBJ_DAYS# = FORMAT(&REPORTS_OBJ_DAYS#,"0000")
:SET &@RH# = "&@REPORTS# &@REPORT_DAYS# &@REPORTS_OBJ# &@REPORTS_OBJ_DAYS#"
:SET &@RH_LEN# = STR_LENGTH(&@RH#)
:SET &@RH_LEN# = FORMAT(&@RH_LEN#,"000000000")

:SET &@VERSIONS# = &VERSIONS#
:SET &@REPORT_DAYS# = FORMAT(&REPORT_DAYS#,"0000")
:SET &@VERSIONS_OBJ# = &VERSIONS_OBJ#
:SET &@VERSIONS_OBJ_DAYS# = FORMAT(&VERSIONS_OBJ_DAYS#,"0000")
:SET &@VC# = "&@VERSIONS# &@REPORT_DAYS# &@VERSIONS_OBJ# &@VERSIONS_OBJ_DAYS#"
:SET &@VC_LEN# = STR_LENGTH(&@VC#)
:SET &@VC_LEN# = FORMAT(&@VC_LEN#,"000000000")

:SET &@AUDIT# = &AUDIT#
:SET &@AUDIT_DAYS# = FORMAT(&AUDIT_DAYS#,"0000")
:SET &@AUDIT_REVISED# = &AUDIT_REVISED#
:SET &@OA# = "&@AUDIT# &@AUDIT_DAYS# &@AUDIT_REVISED#"
:SET &@OA_LEN# = STR_LENGTH(&@OA#)
:SET &@OA_LEN# = FORMAT(&@OA_LEN#,"000000000")

:ATTACH_RES STORE.REORG_SETTINGS, "*", T, Y

:IF &REPLACE# = "TRUE"
:  SET &V_REPLACE# = "REPLACE"
:ELSE
:  SET &V_REPLACE# = "IGNORE"
:ENDIF

:SET &UTILPARMS# = "-I&INIFILE# -B -E&V_REPLACE# -C&CLIENT# -X&$AGENT_RESOURCES_TASK#reorg.uc_data.txt"

! Execute Utility
:INC JOBI.UTILITY.EXECUTE]]></process>
      </row>
