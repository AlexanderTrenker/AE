:INC JOBI.UTILITY.SETUP

:IF &DB_CHANGE# <> ""
:  SET &SCRFILE# = "&OUTDIR#&UTILITY#_&$RESTART_RUNID#.txt"
:  SET &CHG_DATA# = GET_VAR(VARA.UTIL.DB.CHANGE,&DB_CHANGE#,2)
:  SET &SCR# = CREATE_PROCESS(NEW)
:  SET &RET# = PUT_PROCESS_LINE(&SCR#,&CHG_DATA#)
:  SET &RET# = WRITE_PROCESS(&SCR#,"&SCRFILE#",&$AGENT#,&$LOGIN#,OVERWRITE)

:  SET &UTILPARMS# = "-I&INIFILE#  -B&MODE# -1&SCRFILE# -2&S_TRANSPORT# -3&T_TRANSPORT#"

!  Execute Utility
:  INC JOBI.UTILITY.EXECUTE

:ENDIF]]></process>
