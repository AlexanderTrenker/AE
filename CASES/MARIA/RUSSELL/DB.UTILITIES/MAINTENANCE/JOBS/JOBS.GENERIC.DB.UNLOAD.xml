   <scripts>
      <row>
         <process><![CDATA[:SET &UTILITY# = "ucybdbun"
! Common Setup
:INC JOBI.UTILITY.SETUP

:INC JOBI.UTILITY.CREATE_AE_DATA_DIRS

! Utility Specific
:PUT_ATT ARCHIVE_KEY1 = &CLIENT#
:PUT_ATT ARCHIVE_KEY2 = &MODE#

:IF &REMOVE# = "TRUE"
:  SET &V_REMOVE# = "-D"
:ELSE
:  SET &V_REMOVE# = ""
:ENDIF

:IF &RECYCLE# = "TRUE"
:  SET &V_RECYCLE# = "-K"
:ELSE
:  SET &V_RECYCLE# = ""
:ENDIF

:IF &MODE# = "REORG"
:  P Setting LOG_DB_ACTION# to N
:  SET &LOG_DB_ACTION# = "N"
:  SET &UTILPARMS# = "-I&INIFILE# -B&MODE#"
:ELSE
:  SET &V_TRANSPORT# = "-X&TRANSPORT#"
:  SET &UTILPARMS# = "-I&INIFILE# &V_REMOVE# &V_RECYCLE# -B&MODE# -C&CLIENT# &V_TRANSPORT#"
:ENDIF

! Execute Utility
:INC JOBI.UTILITY.EXECUTE]]></process>
      </row>
      <row>
         <post_process>:INC JOBI.UTILITY.POST</post_process>
      </row>
