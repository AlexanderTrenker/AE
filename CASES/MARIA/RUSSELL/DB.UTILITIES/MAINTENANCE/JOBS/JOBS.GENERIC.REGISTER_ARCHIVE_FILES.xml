      <row>
         <process><![CDATA[:PUT_ATT ARCHIVE_KEY1 = &CLIENT#
:PUT_ATT ARCHIVE_KEY2 = &ARCHIVE_RUNID#

:SET &HND# = PREP_PROCESS_REPORT(,&ARCHIVE_RUNID#, "REP", "* U00031145 *","COL=DELIMITER", "DELIMITER=*'*")
:PROCESS &HND#
:  SET &DIR# = GET_PROCESS_LINE(&HND#,2)
:  PRINT "DIR &DIR#"
:  SET &FILES# = PREP_PROCESS_FILENAME(&$AGENT#,"&DIR#*","Y",,,,"UC_LOGIN=&$LOGIN#")
:  PROCESS &FILES#
:    SET &FILE# = GET_PROCESS_LINE(&FILES#)
:    PRINT "FILE &FILE#"
:    REGISTER_OUTPUTFILE "&FILE#", "N"
:  ENDPROCESS
:  CLOSE_PROCESS &FILES#
:ENDPROCESS
:CLOSE_PROCESS &HND#]]></process>
