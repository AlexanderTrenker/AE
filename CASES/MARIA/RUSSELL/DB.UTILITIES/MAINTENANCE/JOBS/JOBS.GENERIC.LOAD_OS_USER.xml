   <scripts>
      <row>
         <process><![CDATA[:SET &UTILITY# = "ucybdbld"
! Common Setup
:INC JOBI.UTILITY.SETUP

! Utility Specific
:SET &OS_USER# = GET_ATT(LOGIN_INFO)
:SET &OS_USER# = STR_UC(&OS_USER#)

! Check for windows domain and strip off
:SET &POS# = STR_FIND(&OS_USER#, '\')
:IF &POS# > 0
:  SET &POS# = &POS# + 1
:  SET &OS_USER# = STR_CUT(&OS_USER#,&POS#)
:ENDIF

:P OS_USER: &OS_USER#
:SET &OS_USER_LIKE# = "&OS_USER#/%"

:SET &UHND# = PREP_PROCESS_VAR(VARA.SEC_SQLI.GET_UTILITY_USER)
:SET &USER_CNT# = GET_PROCESS_INFO(&UHND#,ROWS)
:IF &USER_CNT# > 0
:  P &LF#&LF#*** Found The following matching users in Client &CLIENT# ***:
:  PROCESS &UHND#
:    SET &USR# = GET_PROCESS_LINE(&UHND#,1)
:    P &USR#
:  ENDPROCESS
:  CLOSE_PROCESS &UHND#
:  P &LF#***Skipping user load.. ***
:  STOP NOMSG
:ENDIF
:CLOSE_PROCESS &UHND#

:PUT_ATT ARCHIVE_KEY1 = &CLIENT#
:PUT_ATT ARCHIVE_KEY2 = &OS_USER#

:ATTACH_RES STORE.UTILITY.USER, "*", T, Y

:SET &V_REPLACE# = "IGNORE"

:SET &UTILPARMS# = "-I&INIFILE# -B -E&V_REPLACE# -C&CLIENT# -X&$AGENT_RESOURCES_TASK#UTIL.USER.txt"

! Execute Utility
:INC JOBI.UTILITY.EXECUTE]]></process>
      </row>
   </scripts>
