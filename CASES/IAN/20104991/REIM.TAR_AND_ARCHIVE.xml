   </general_attributes>
   <scripts>
      <row>
         <process><![CDATA[. &REIM_PROFILE
cd &SDIR
echo "&SDIR"
echo "Listado de archivos en &SDIR"
ls -lrt
echo "Empaquetando archivos"
!tar -czvf - ./* | gzip > &TDIR#/&TFILE# && rm -f &SDIR/*
cd &SDIR
tar -cvf - ./* | gzip > &TDIR#/&TFILE#
cd &SDIR
mv ./* /awabatch/oracle/REIM/BCK_ARCH/
err=$?
exit $err]]></process>
      </row>
