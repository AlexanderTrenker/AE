   <scripts>
      <row>
         <process><![CDATA[:PRINT "Backup Report: &UC4RB_BACKUP_REPORT#"
:SET &UC4RB_WAS_SERVER_NAME#=''
:SET &UC4RB_WAS_NODE_NAME#=''
:SET &HND# = PREP_PROCESS_FILE(&AGENT#, &UC4RB_BACKUP_REPORT#, "*::stopped*")
:PROCESS &HND#
:  SET &UC4RB_RESULT_LINE# = GET_PROCESS_LINE(&HND#)
:  DEFINE &server_status#, string, 3
:  FILL &server_status#[] = STR_SPLIT(&UC4RB_RESULT_LINE#,"::")
:  SET &server# = STR_TRIM(&server_status#[2])
:  SET &node# = STR_TRIM(&server_status#[1])
:  SET &UC4RB_WAS_SERVER_NAME# = STR_CAT(&UC4RB_WAS_SERVER_NAME#, &server#)
:  SET &UC4RB_WAS_SERVER_NAME# = STR_CAT(&UC4RB_WAS_SERVER_NAME#,",")
:  SET &UC4RB_WAS_NODE_NAME# = STR_CAT(&UC4RB_WAS_NODE_NAME#,&node#)
:  SET &UC4RB_WAS_NODE_NAME# = STR_CAT(&UC4RB_WAS_NODE_NAME#,",")
:ENDPROCESS
:CLOSE_PROCESS &HND#
:SET &svr_length# = STR_LENGTH ( &UC4RB_WAS_SERVER_NAME# )
:IF &svr_length# > 1
:  SET &svr_length# = &svr_length# - 1
:  SET &UC4RB_WAS_SERVER_NAME# = SUBSTR(&UC4RB_WAS_SERVER_NAME#, 1, &svr_length#)
:  SET &node_length# = STR_LENGTH ( &UC4RB_WAS_NODE_NAME# )
:  SET &node_length# = &node_length# - 1
:  SET &UC4RB_WAS_NODE_NAME# = SUBSTR(&UC4RB_WAS_NODE_NAME#, 1, &node_length#)
:  PRINT "List of previously stopped servers:"
:  PRINT "UC4RB_WAS_SERVER_NAMES: &UC4RB_WAS_SERVER_NAME#"
:  PRINT "UC4RB_WAS_NODE_NAMES: &UC4RB_WAS_NODE_NAME#"
:ELSE
:  PRINT "Server status has not been changed."
:  SET &UC4RB_WAS_SERVER_NAME# = ""
:ENDIF
]]></process>
      </row>
